{
  "name": "craft3-seomatic",
  "version": "3.0.0",
  "description": "SEOmatic plugin for Craft 3",
  "main": "gulpfile.js",
  "author": "Andrew Welch, nystudio107 <andrew@nystudio107.com>",
  "copyright": "nystudio107",
  "license": "proprietary",
  "private": true,
  "paths": {
    "src": {
      "base": "./src/",
      "css": "./src/css/",
      "fontello": "./src/fontello/",
      "fonts": "./src/fonts/",
      "json": "./src/json/",
      "js": "./src/js/",
      "img": "./src/img/",
      "scss": "./src/scss/"
    },
    "dist": {
      "base": "./public/",
      "css": "./public/css/",
      "js": "./public/js/",
      "img": "./public/img/"
    },
    "build": {
      "base": "./build/",
      "css": "./build/css/",
      "js": "./build/js/",
      "html": "./build/html/",
      "img": "./build/img/"
    },
    "scss": [
    ]
  },
  "vars": {
    "siteCssName": "site.combined.min.css",
    "scssName": "style.scss",
    "cssName": "style.css"
  },
  "globs": {
    "distCss": [
      "./build/css/*.css",
      "./src/css/*.css"
    ],
    "img": [
      "./public/img/"
    ],
    "components": [
      "./src/components/**/*.vue"
    ],
    "fonts": [
    ],
    "distJs": [
      "./build/js/*.js",
      "./node_modules/vue/dist/vue.min.js",
      "./node_modules/vue-resource/dist/vue-resource.min.js"
    ]
  },
  "dependencies": {
    "vue": "^2.2.0",
    "vue-resource": "^1.0.3"
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "fancy-log": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-debug": "^2.1.2",
    "gulp-header": "^1.8.7",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.3.0",
    "gulp-newer": "^1.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-print": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-sass": "^2.1.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.2.1",
    "gulp-uglify": "^1.5.4",
    "moment": "^2.14.1"
  },
  "scripts": {
    "start": "gulp",
    "build": "gulp build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nystudio107/craft3-seomatic.git"
  },
  "bugs": {
    "email": "andrew@nystudio107.com"
  }
}
